	volume = {56},
	year = {2001}}

@article{Drummond2001QuantumI,
	abstract = {We analyze the quantum dynamics of radiation propagating in a single-mode optical fiber with dispersion, nonlinearity, and Raman coupling to thermal phonons. We start from a fundamental Hamiltonian that includes the principal known nonlinear effects and quantum-noise sources, including linear gain and loss. Both Markovian and frequency-dependent, non-Markovian reservoirs are treated. This treatment allows quantum Langevin equations, which have a classical form except for additional quantum-noise terms, to be calculated. In practical calculations, it is more useful to transform to Wigner or $+$P quasi-probability operator representations. These transformations result in stochastic equations that can be analyzed by use of perturbation theory or exact numerical techniques. The results have applications to fiber-Opt. Commun., networking, and sensor technology.},
	author = {Drummond, P. D. and Corney, J. F.},
	date-added = {2023-02-11 15:31:34 +1100},
	date-modified = {2023-02-11 15:31:34 +1100},
